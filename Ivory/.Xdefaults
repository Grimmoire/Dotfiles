# URxvt.depth: 32
URxvt.scrollBar: false
URxvt.font: xft:M+ 1mn regular for Powerline:pixelsize=11,\
            [codeset=JISX0208]xft:Kochi Gothic:pixelsize=9:antialias=false,\
            [codeset=KSC5601]xft:Baekmuk Dotum:pixelsize=9:antialias=false
URxvt.boldFont: xft:M+ 1mn Bold:pixelsize=11,\
                [codeset=JISX0208]xft:Kochi Gothic:pixelsize=11,\
                [codeset=KSC5601]xft:Baekmuk Dotum:pixelsize=11:antialias=false

URxvt*termName: rxvt-256color
URxvt*perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt*url-select.launcher: firefox
URxvt*url-select.underline: true
URxvt*matcher.button: 1
URxvt*colorUL: #CC4141
URxvt*shading: true

urxvt*foreground: #333333

urxvt*background: #F5F5E9



!black
